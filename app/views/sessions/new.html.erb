<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
<%= notice %>
<%= form_for :session do |f| %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>
  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<p>New user? <%= link_to "Sign up now!", new_user_path %></p>
<% end %>